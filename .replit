modules = ["python-3.11", "nodejs-20"]
[agent]
expertMode = true
integrations = ["python_anthropic:1.0.0", "python_openai:1.0.0", "python_gemini:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["gitFull", "playwright-driver", "xorg.libxcb", "xorg.libX11", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXfixes", "mesa", "cups", "nspr", "nss", "dbus", "at-spi2-core", "pango", "cairo", "alsa-lib", "libxkbcommon"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "cli-tool"

[[workflows.workflow]]
name = "cli-tool"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python example.py"

[workflows.workflow.metadata]
outputType = "console"
