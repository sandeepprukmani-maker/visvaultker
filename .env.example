# AI Model Configuration
GOOGLE_AI_API_KEY=your_gemini_api_key_here
GOOGLE_API_KEY=your_gemini_api_key_here

# Custom Model Configuration (optional)
CUSTOM_MODEL_API_KEY=your_custom_model_api_key
CUSTOM_MODEL_BASE_URL=https://your-api-endpoint.com/v1

# OAuth Configuration (for dynamic token fetching)
OAUTH_TOKEN_URL=https://your-oauth-server.com/oauth/token
OAUTH_CLIENT_ID=your_client_id
OAUTH_CLIENT_SECRET=your_client_secret
OAUTH_GRANT_TYPE=client_credentials
OAUTH_SCOPE=your_scope
GW_BASE_URL=https://your-gateway-base-url.com/v1

# BrowserBase (optional - for cloud browser automation)
BROWSERBASE_API_KEY=your_browserbase_key
BROWSERBASE_PROJECT_ID=your_project_id

# PostgreSQL Database (optional - uses Python/SQLite if not provided)
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Environment
NODE_ENV=development
