# Database Configuration (PostgreSQL)
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/visionvault
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=your_password
PGDATABASE=visionvault

# AI Model Provider API Keys
# Choose one based on your preferred model provider

# Google Gemini (for google/gemini models) - Recommended
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key_here
# Get your key at: https://makersuite.google.com/app/apikey

# OpenAI (for openai/gpt models)
OPENAI_API_KEY=your_openai_api_key_here
# Get your key at: https://platform.openai.com/api-keys

# Anthropic (for anthropic/claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# BrowserBase (optional - for cloud browsers instead of local)
# BROWSERBASE_API_KEY=your_browserbase_api_key_here
# BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# Environment
NODE_ENV=development
