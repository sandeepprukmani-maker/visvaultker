# OAuth Gen AI Gateway Configuration
# Set USE_OAUTH_GATEWAY=true to use the gateway (default)
# Set USE_OAUTH_GATEWAY=false to use direct API keys instead
USE_OAUTH_GATEWAY=true

# OAuth Configuration (Required when USE_OAUTH_GATEWAY=true)
OAUTH_TOKEN_URL=https://your-oauth-server.com/oauth/token
OAUTH_CLIENT_ID=your-client-id
OAUTH_CLIENT_SECRET=your-client-secret
OAUTH_GRANT_TYPE=client_credentials
OAUTH_SCOPE=your-scope

# Gen AI Gateway Base URL (Required when USE_OAUTH_GATEWAY=true)
GW_BASE_URL=https://your-gateway.com/v1

# Direct API Keys (Only used when USE_OAUTH_GATEWAY=false)
# Uncomment and fill these if you want to use direct API access
# ANTHROPIC_API_KEY=your-anthropic-key
# OPENAI_API_KEY=your-openai-key
# GEMINI_API_KEY=your-gemini-key
